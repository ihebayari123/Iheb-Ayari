   <string notr="true">/* Fenêtre principale */
QDialog {
	
	background-image: url(:/img/images/104016.jpg);

    background-color: #2c3e50; /* Bleu foncé */
    color: #ecf0f1; /* Blanc cassé */
    border-radius: 10px;
    padding: 20px;
}

/* Champs de saisie (QLineEdit) */
QLineEdit {
    background-color: #34495e; /* Bleu-gris */
    color: #ecf0f1; /* Blanc cassé */
    border: 2px solid rgb(0, 74, 173); /* Bleu */
    border-radius: 5px;
    padding: 5px;
    font-size: 14px;
}

QLineEdit:focus {
    border: 2px solid rgb(0, 74, 173); /* Bleu */
    background-color: #3b5998; /* Bleu plus clair */
}

/* Bouton de connexion (QPushButton) */
QPushButton {
    background-color: rgb(0, 74, 173); /* Bleu */
    color: #ecf0f1; /* Blanc cassé */
    border: none;
    border-radius: 5px;
    padding: 10px;
    font-size: 16px;
    font-weight: bold;
    transition: background-color 0.3s ease;
}

QPushButton:hover {
    background-color: #004a9d; /* Bleu légèrement plus foncé */
}

QPushButton:pressed {
    background-color: #003a7d; /* Bleu encore plus foncé */
}

/* Labels (QLabel) */
QLabel {
    color: #ecf0f1; /* Blanc cassé */
    font-size: 14px;
    font-weight: bold;
}

/* Placeholder text dans QLineEdit */
QLineEdit[placeholderText] {
    color: #95a5a6; /* Gris clair */
}

/* MessageBox */
QMessageBox {
    background-color: #34495e; /* Bleu-gris */
    color: #ecf0f1; /* Blanc cassé */
    border-radius: 10px;
}

</string>
